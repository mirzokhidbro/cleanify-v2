{
    "name": "cleanify/api-v2",
    "description": "Cleanify REST API v2",
    "type": "project",
    "require": {
        "php": "^8.1",
        "laminas-api-tools/api-tools": "^1.7",
        "laminas/laminas-component-installer": "^3.4",
        "laminas/laminas-mvc": "^3.6",
        "doctrine/doctrine-orm-module": "^5.3",
        "slim/slim": "^4.14",
        "slim/psr7": "^1.7",
        "vlucas/phpdotenv": "^5.6",
        "symfony/cache": "^5.4",
        "doctrine/migrations": "^3.8",
        "php-di/php-di": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "laminas/laminas-component-installer": true,
            "laminas/laminas-dependency-plugin": true
        }
    }
}
